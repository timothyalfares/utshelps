<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="150px"
        android:text="Setting"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        android:paddingLeft="160sp"
        android:paddingTop="10sp"
        android:id="@+id/textView"
        android:background="#6233D6"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="1600px"
        android:layout_height="400px"
        android:background="#C9C9C9"
        android:id="@+id/textView4"
        android:layout_alignTop="@+id/homebtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="200px"
        android:layout_height="220px"
        android:id="@+id/homebtn"
        android:src="@drawable/home2"
        android:layout_marginLeft="230px"
        android:layout_marginTop="1850px"
        android:background="@null"/>

    <ImageButton
        android:layout_width="200px"
        android:layout_height="200px"
        android:id="@+id/conbtn"
        android:src="@drawable/phone3"
        android:layout_marginLeft="650px"
        android:layout_marginTop="1880px"
        android:background="@null"/>


    <ImageButton
        android:layout_width="250px"
        android:layout_height="250px"
        android:id="@+id/setbtn"
        android:src="@drawable/set2"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="1050px"
        android:layout_marginTop="1860px"
        android:background="@null"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Profile"
        android:layout_marginLeft="295px"
        android:layout_marginTop="310px"
        android:id="@+id/textView5" />

    <ImageView
        android:layout_width="120px"
        android:layout_height="120px"
        android:id="@+id/imageView7"
        android:src="@drawable/stu2"
        android:layout_marginLeft="160px"
        android:layout_marginTop="280px"/>

    <View
        android:id="@+id/UnderLine"
        android:layout_width="320dp"
        android:layout_height="1dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="125dp"
        android:background="#000000"
        android:paddingBottom="20px"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Log out"
        android:layout_marginTop="31dp"
        android:layout_below="@+id/UnderLine"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:id="@+id/textView6" />

    <ImageView
        android:layout_width="120px"
        android:layout_height="120px"
        android:id="@+id/imageView8"
        android:src="@drawable/logout"
        android:layout_marginLeft="160px"
        android:layout_marginTop="525px" />

    <View
        android:id="@+id/UnderLine2"
        android:layout_width="320dp"
        android:layout_height="1dp"
        android:layout_marginTop="200dp"
        android:background="#000000"
        android:paddingBottom="20px"
        android:layout_centerHorizontal="true"/>

    <ImageButton
        android:layout_width="200px"
        android:layout_height="220px"
        android:id="@+id/profbtn"
        android:src="@drawable/left"
        android:layout_alignBottom="@+id/UnderLine2"
        android:layout_alignLeft="@+id/outbtn"
        android:layout_alignStart="@+id/outbtn"
        android:background="@null"/>

    <ImageButton
        android:layout_width="200px"
        android:layout_height="220px"
        android:id="@+id/outbtn"
        android:src="@drawable/left"
        android:background="@null"
        android:layout_alignBottom="@+id/UnderLine"
        android:layout_alignRight="@+id/setbtn"
        android:layout_alignEnd="@+id/setbtn" />
</RelativeLayout>