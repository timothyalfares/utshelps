<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <Button
        android:layout_width="205dp"
        android:layout_height="165dp"
        android:text="My Profile"
        android:textColor="#FFFFFF"
        android:paddingTop="200px"
        android:id="@+id/profilebtn"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#9DB0C4"
        />
    <Button
        android:layout_width="205dp"
        android:layout_height="165dp"
        android:paddingTop="200px"
        android:text="My Booking"
        android:textColor="#FFFFFF"
        android:id="@+id/bookbtn"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#6D9FC7"/>
    <Button
        android:layout_width="200dp"
        android:layout_height="175dp"
        android:text="View Session"
        android:paddingTop="300px"
        android:textColor="#FFFFFF"
        android:id="@+id/viewbtn"
        android:layout_below="@+id/profilebtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#4658C2"
        />
    <Button
        android:layout_width="200dp"
        android:layout_height="188dp"
        android:text="Booking History"
        android:paddingTop="300px"
        android:textColor="#FFFFFF"
        android:id="@+id/historybtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/viewbtn"
        android:background="#33437A"/>

    <ImageView
        android:layout_width="250px"
        android:layout_height="250px"
        android:id="@+id/imageView3"
        android:src="@drawable/stu2"
        android:elevation="2dp"
        android:layout_alignTop="@+id/imageView4"
        android:layout_toLeftOf="@+id/imageView5"
        android:layout_toStartOf="@+id/imageView5" />
    <ImageView
        android:layout_width="250px"
        android:layout_height="250px"
        android:id="@+id/imageView4"
        android:src="@drawable/book2"
        android:elevation="2dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView5"
        android:layout_toEndOf="@+id/imageView5"
        android:layout_marginTop="29dp" />
    <ImageView
        android:layout_width="450px"
        android:layout_height="400px"
        android:id="@+id/imageView5"
        android:src="@drawable/view"
        android:elevation="2dp"
        android:layout_below="@+id/profilebtn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="29dp" />
" />
    <ImageView
        android:layout_width="450px"
        android:layout_height="350px"
        android:id="@+id/imageView6"
        android:src="@drawable/history"
        android:elevation="2dp"
        android:layout_alignTop="@+id/historybtn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="24dp" />

    <TextView
        android:layout_width="1600px"
        android:layout_height="400px"
        android:background="#C9C9C9"
        android:id="@+id/textView4"
        android:layout_alignTop="@+id/homebtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="200px"
        android:layout_height="220px"
        android:id="@+id/homebtn"
        android:src="@drawable/home2"
        android:layout_marginLeft="230px"
        android:layout_marginTop="1850px"
        android:background="@null"/>

    <ImageButton
        android:layout_width="200px"
        android:layout_height="200px"
        android:id="@+id/conbtn"
        android:src="@drawable/phone3"
        android:layout_marginLeft="650px"
        android:layout_marginTop="1880px"
        android:background="@null"/>

    <ImageButton
        android:layout_width="250px"
        android:layout_height="250px"
        android:id="@+id/setbtn"
        android:src="@drawable/set2"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="1050px"
        android:layout_marginTop="1860px"
        android:background="@null"/>

</RelativeLayout>